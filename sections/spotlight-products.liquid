{{ 'component-spotlight-products.css' | asset_url | stylesheet_tag }}
<style>
    {%- liquid
        assign product_bg = section.settings.product_bg
        assign product_bg_gradient = section.settings.product_bg_gradient
        assign product_bg_image = section.settings.product_bg_image

        assign color_title = section.settings.color_title
        assign fontsize_title = section.settings.fontsize_title
        assign fontsize_title_tablet = section.settings.fontsize_title_tablet
        assign fontsize_title_mb = section.settings.fontsize_title_mb
        assign product_button_width = section.settings.product_button_width

        assign mg_bottom_title = section.settings.mg_bottom_title
        assign mg_bottom_title_mb = section.settings.mg_bottom_title_mb
        assign mg_bottom_view = section.settings.mg_bottom_view
        assign mg_bottom_view_mb = section.settings.mg_bottom_view_mb

        assign color_view_all = section.settings.color_view_all
        assign color_view_all_icon = section.settings.color_view_all_icon
        assign font_weight_view_all = section.settings.font_weight_view_all

        assign container = section.settings.container
        assign padding_full_width = section.settings.padding_full_width

        assign mg_top_desktop = section.settings.mg_top_desktop
        assign mg_top_tablet = section.settings.mg_top_tablet
        assign mg_top_mobile = section.settings.mg_top_mobile

        assign mg_bottom_desktop = section.settings.mg_bottom_desktop
        assign mg_bottom_tablet = section.settings.mg_bottom_tablet
        assign mg_bottom_mobile = section.settings.mg_bottom_mobile

        assign block_title = section.settings.product_block_title
        assign block_title_align = section.settings.title_align
        assign enable_heading_sticky = section.settings.enable_heading_sticky
        assign headingLineHeight = settings.type_heading_line_height
    -%}

    {%- if product_bg_image != blank -%}
        .section-block-{{section.id}}{background: url({{ product_bg_image | img_url: '1920x' }});background-repeat: repeat-x;background-size: cover}
        .section-block-{{section.id}} .halo-block-header .title .text {background-color: transparent}
    {%- else -%}
        .section-block-{{section.id}},.section-block-{{section.id}} .halo-block-header .title .text {background: {% if product_bg_gradient != blank %}{{ product_bg_gradient }}{% else %}{{ product_bg }}{% endif %}}
    {%- endif -%}
    {%- if section.settings.view_all != blank -%}
        .section-block-{{section.id}} .halo-block-header{margin-bottom: {{ mg_bottom_title | append: 'px' }}}
    {%- else -%}
        .section-block-{{section.id}} .halo-block-header{margin-bottom: 0}
    {%- endif -%}
    .section-block-{{section.id}} .halo-block-header .title {color: {{ color_title }};font-size: {{ fontsize_title | append: 'px'  }};
        {%- if section.settings.view_all != blank -%}
            margin-bottom: 0;
        {%- else -%}
            margin-bottom: {{ mg_bottom_title | append: 'px' }};
        {%- endif -%}
        {%- if fontsize_title > headingLineHeight -%}
            line-height: {{ fontsize_title | append: 'px' }};
        {%- endif -%}
    }
    .section-block-{{section.id}} .halo-block-header .title:before {display: none}
    .section-block-{{section.id}} .halo-block-header .view_all {color: {{color_view_all}};font-weight: {{ font_weight_view_all }};margin-bottom: {{ mg_bottom_view | append: 'px' }} !important;
        {%- unless section.settings.enable_border_view_all -%}
            border: 0
        {%- endunless -%}
    }
    .section-block-{{section.id}} .halo-block-header .view_all svg {color: {{color_view_all_icon}};fill: {{color_view_all_icon}}}
    @media (max-width: 1024px) {
        .section-block-{{section.id}} .halo-block-header .title {font-size: {{ fontsize_title_tablet | append: 'px'  }};
            {%- if fontsize_title_tablet > headingLineHeight -%}
                line-height: {{ fontsize_title_tablet | append: 'px' }};
            {%- endif -%}
        }
    }
    @media (max-width: 991px) {
        .section-block-{{section.id}} .halo-block-header .title {
            {%- if section.settings.view_all != blank -%}
                margin-bottom: 10px;
                display: block;
            {%- endif -%}
        }
        .section-block-{{section.id}} .halo-block-header {margin-bottom: {{ mg_bottom_title_mb | append: 'px' }}}
        .section-block-{{section.id}} .halo-block-header .view_all {margin-bottom: {{ mg_bottom_view_mb | append: 'px' }} !important}
    }
    @media (max-width: 767px) {
        .section-block-{{section.id}} {
            padding-top: {{ mg_top_mobile | append: 'px' }};padding-bottom: {{ mg_bottom_mobile | append: 'px' }};
        }
        {%- if container == 'full' -%}
            .section-block-{{section.id}} .container-full {padding-left: 10px;padding-right: 10px}
        {%- endif -%}
        .section-block-{{section.id}} .halo-block-header .title {font-size: {{ fontsize_title_mb | append: 'px'  }};
            {%- if fontsize_title_mb > headingLineHeight -%}
                line-height: {{ fontsize_title_mb | append: 'px' }};
            {%- endif -%}
            {%- if section.settings.view_all != blank -%}
                margin-bottom: 10px;
                display: block;
            {%- endif -%}
        }
        {%- if mg_bottom_title == 84 -%}
            .section-block-{{section.id}} .halo-block-header .title .text{letter-spacing: -.02em}
        {%- endif -%}
    }
    @media (min-width: 1200px) {
        .section-block-{{section.id}} {
            padding-top: {{ mg_top_desktop | append: 'px' }};padding-bottom: {{ mg_bottom_desktop | append: 'px' }};
        }
        {%- if enable_heading_sticky -%}
            .section-block-{{section.id}} .container-full ,
            .template-index .wrapper-body {overflow: clip;}
            .section-block-{{section.id}} .halo-block-header .title .text {background-color: transparent}
        {%- endif -%}
    }
    @media (min-width: 1025px) {
        {%- if container == 'full' -%}
            .section-block-{{section.id}} .container-full .halo-block-header {padding-left: {{ padding_full_width | append: 'px' }};padding-right: {{ padding_full_width | append: 'px' }} !important}
            .section-block-{{section.id}} .container-full .spotlight-products__wrapper {padding-left: {{ padding_full_width | append: 'px' }};padding-right: 0}
            body.layout_rtl .section-block-{{section.id}} .container-full .spotlight-products__wrapper {padding-right: {{ padding_full_width | append: 'px' }};padding-left: 0}
        {%- endif -%}
    }
    @media (min-width: 768px) and (max-width: 1199px) {
        .section-block-{{section.id}} {
            padding-top: {{ mg_top_tablet | append: 'px' }};padding-bottom: {{ mg_bottom_tablet | append: 'px' }};
        }
        {%- if container == 'full' -%}
            .section-block-{{section.id}} .container-full {padding-left: 15px;padding-right: 15px}
        {%- endif -%}
    }
    @media (min-width: 991px) and (max-width: 1024px) {
        {%- if mg_bottom_title == 84 -%}
            .section-block-{{section.id}} .halo-block-header{ margin-bottom: 35px }
        {%- endif -%}
    }
</style>
<div class="spotlight-products section-block-{{section.id}}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="--p-top: {{ section.settings.padding_top }}px; --p-bottom: {{ section.settings.padding_bottom }}px">
    <div class="{% if container == 'container' %}container{% else %}container-{{ container }}{% endif %}">
        {%- if block_title != blank -%}
            <div class="halo-block-header text-{{ block_title_align }}{% if section.settings.view_all != blank %} viewAll--{{ section.settings.view_all_align }} has-viewAll{% endif %}{% if enable_heading_sticky %} enable_heading_sticky{% endif %}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                <h3 class="title"><span class="text">{{ block_title | escape }}</span></h3>
                {%- if section.settings.view_all != blank -%}
                <a class="view_all"{% if section.settings.link_view_all == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.link_view_all }}"{% endif %} title="{{section.settings.view_all}}">{%- if section.settings.icon_view_all -%}<span class="icon-svg">{{ section.settings.icon_view_all }}</span>{%- endif -%}{{ section.settings.view_all }}</a>
                {%- endif -%}
            </div>
        {%- endif -%}
        <div class="spotlight-products__wrapper">
            <div class="spotlight-products__item spotlight-products__item--product{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                <div class="spotlight-products__product" data-spotlight-product>
                    <div class="spotlight-products__products--slider" data-item-dots="{{ section.settings.product_block_dots }}" data-item-dots-mb="{{ section.settings.product_block_dots_mb }}" data-item-arrows="{{ section.settings.product_block_arrows }}" data-item-arrows-mb="{{ section.settings.product_block_arrows_mb }}">
                        {%- for block in section.blocks -%}
                            {%- liquid 
                                assign product = block.settings.product
                            -%}
                            {%- if product != nil -%}
                                <div class="product">
                                    {% render 'product-grid-layout',
                                        product_card_product: product,
                                        show_add_to_cart: true,
                                        serial: forloop.index,
                                        sectionId: section.id,
                                        show_list: true,
                                        loading: true
                                    %}  
                                </div>
                            {%- else -%}
                                <div class="product">
                                    {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
                                    {% render 'product-card-placeholder', index: current %}
                                </div>
                            {%- endif -%}
                        {%- endfor -%}
                    </div>
                </div>
            </div>
            
            <div class="spotlight-products__item spotlight-products__item--lookbook_banner p-relative{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                <div class="spotlight-products__lookbook">
                    <div class="spotlight-products__lb_item d-inline-block w-100{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} o-hidden{% endif %}">
                        {%- if section.settings.image_1 != blank -%}
                            {% if section.settings.animations_hover_elements == '3d-lift' or section.settings.animations_hover_elements == 'vertical-lift' %}
                            <div class="image-animation">
                            {% endif %}
                                <div class="lookbook-img p-relative image-zoom image adaptive_height" style="padding-top: {% unless section.settings.image_1.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: section.settings.image_1.aspect_ratio | times: 200 | append: '%' }}{% else %}{{ 1 | divided_by: section.settings.image_1.aspect_ratio | times: 100 | append: '%' }}{% endunless %}">
                                    <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{%- if section.settings.image_1.width < 320 -%}{{ section.settings.image_1 | image_url: width: section.settings.image_1.width }} 320w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 320 -%}{{ section.settings.image_1 | image_url: width: 375 }} 375w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 750 -%}{{ section.settings.image_1 | image_url: width: 750 }} 750w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 1100 -%}{{ section.settings.image_1 | image_url: width: 1100 }} 1100w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 1370 -%}{{ section.settings.image_1 | image_url: width: 1370 }} 1370w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 1500 -%}{{ section.settings.image_1 | image_url: width: 1500 }} 1500w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 1780 -%}{{ section.settings.image_1 | image_url: width: 1780 }} 1780w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 2000 -%}{{ section.settings.image_1 | image_url: width: 2000 }} 2000w,{%- endif -%}
                                        {%- if section.settings.image_1.width >= 2800 -%}{{ section.settings.image_1 | image_url: width: 2800 }} 2800w{%- endif -%}"
                                        sizes="50vw"{% unless settings.enable_lazyload %} src="{{ section.settings.image_1 | image_url: width: 750 }}"{% endunless %} alt="{{ section.settings.image_1.alt | escape }}" width="{{ section.settings.image_1.width }}" height="{{ section.settings.image_1.width | divided_by: section.settings.image_1.aspect_ratio | round}}" 
                                        {%- if settings.enable_lazyload -%}class="lazyload" loading="lazy"{%- endif -%}
                                        {% unless section.settings.image_1.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ section.settings.image_1.presentation.focal_point }}"{% endunless %}
                                    >
                                    {%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%- endif -%}

                                    <div class="spotlight-products__product_point">
                                        {%- for block in section.blocks -%}
                                            <div class="point-button select-icon{% if forloop.first %} active{% endif %}" data-slide="{{ forloop.index }}" style="--left-position: {{ block.settings.horizontal_pos }}%; --top-position: {{ block.settings.vertical_pos }}%; --icon_color: {{block.settings.icon_color}}">
                                                <span class="glyphicon"></span>
                                            </div>
                                        {%- endfor -%}
                                    </div>
                                </div>
                            {% if section.settings.animations_hover_elements == '3d-lift' or section.settings.animations_hover_elements == 'vertical-lift' %}    
                            </div>
                            {% endif %}
                        {%- else -%}
                            <div class="not_img" style="--height_not_img: 1088px">1436 x 1088px</div>
                        {%- endif -%}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
